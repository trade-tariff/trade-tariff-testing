{
  "name": "trade-tariff-testing",
  "version": "1.0.0",
  "description": "E2E testing of the Online Trade Tariff",
  "dependencies": {
    "@jc21/cypress-jsonschema-validation": "^0.0.1",
    "@jc21/cypress-swagger-validation": "^0.1.0",
    "ansi-regex": "^6.0.1",
    "ansi-styles": "^6.1.1",
    "csv-parse": "^5.3.0",
    "cypress-dotenv": "https://github.com/morficus/cypress-dotenv",
    "cypress-tags": "^1.0.2",
    "glob-parent": "^6.0.2",
    "nanoid": "^4.0.0",
    "postcss": "^8.4.17",
    "set-value": "^4.1.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "allure-commandline": "^2.18.1",
    "cypress": "10.10.0",
    "cypress-dark": "^1.7.14",
    "cypress-file-upload": "^5.0.8",
    "cypress-fill-command": "^1.0.1",
    "cypress-grep": "^3.0.3",
    "cypress-mochawesome-reporter": "^3.2.3",
    "cypress-utils": "^3.0.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.25.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-cypress": "^2.11.3",
    "flow-bin": "^0.188.2",
    "mocha": "^10.0.0",
    "mocha-allure-reporter": "^1.4.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^6.0.1"
  },
  "scripts": {
    "open:dev": "CYPRESS_BASE_URL=https://dev.trade-tariff.service.gov.uk CYPRESS_SPACE=DEVELOPMENT npx cypress open",
    "open:staging": "CYPRESS_BASE_URL=https://staging.trade-tariff.service.gov.uk CYPRESS_SPACE=STAGING npx cypress open",
    "open:prod": "CYPRESS_BASE_URL=https://www.trade-tariff.service.gov.uk CYPRESS_SPACE=PRODUCTION npx cypress open",
    "clean:reports": "rm -R -f cypress/reports && mkdir cypress/reports && mkdir cypress/reports/mochareports",
    "pretests": "npm run clean:reports",
    "scripts": "cypress run",
    "smoketests": "cypress run --spec \"cypress/e2e/**/smokeTest*.cy.js\" --browser chrome --headless",
    "runsmoketests": "npm run pretests && (npm run smoketests || npm run posttests)",
    "alltests": "cypress run",
    "combine-reports": "mochawesome-merge cypress/reports/mocha/*.json > cypress/reports/mochareports/report.json",
    "generate-report": "marge cypress/reports/mochareports/report.json -f report -o cypress/reports/mochareports",
    "posttests": "npm run combine-reports && npm run generate-report",
    "tests": "npm run pretests && (npm run alltests || npm run posttests)",
    "regressiontests": "npx cypress run --spec \"cypress/e2e/**/*.cy.js\" --env baseUrl=https://www.trade-tariff.service.gov.uk",
    "xitests": "npx cypress run --spec \"cypress/e2e/XI/**/*.cy.js\"",
    "uktests": "npx cypress run --spec \"cypress/e2e/UK/**/*.cy.js\"",
    "dctests": "cypress run --spec \"/*/**/DutyCalculator/**/*spec.js\" --browser chrome --headless --reporter mocha-allure-reporter",
    "report:allure": "allure generate allure-results --clean -o allure-report && allure open allure-report",
    "test:allure": "npm run dctests && npm run report:allure",
    "open:localhost": "CYPRESS_BASE_URL=http://localhost:3001 npx cypress open",
    "tags:roo": "CYPRESS_BASE_URL=https://staging.trade-tariff.service.gov.uk yarn run cypress run --env=grepTags=roo-tag,grepFilterSpecs=true",
    "tags:unreleased": "CYPRESS_BASE_URL=https://staging.trade-tariff.service.gov.uk yarn run cypress run --env=grepTags=unreleased-tag,grepFilterSpecs=true",
    "tags:xbrowser": "CYPRESS_BASE_URL=https://staging.trade-tariff.service.gov.uk yarn run cypress run --env=grepTags=xbrowser-tag,grepFilterSpecs=true",
    "open:reviewapp": "bin/cypress-review-app.sh",
    "staging-tariff-frontend-smoketests": "CYPRESS_SPACE=STAGING CYPRESS_BASE_URL=https://staging.trade-tariff.service.gov.uk yarn run cypress run --spec cypress/e2e/HOTT-Shared/smokeTestCI.cy.js",
    "staging-tariff-backend-smoketests": "CYPRESS_SPACE=STAGING CYPRESS_BASE_URL=https://staging.trade-tariff.service.gov.uk yarn run cypress run --spec cypress/e2e/HOTT-Shared/smokeTestCI.cy.js",
    "staging-tariff-duty-calculator-smoketests": "CYPRESS_SPACE=STAGING CYPRESS_BASE_URL=https://staging.trade-tariff.service.gov.uk yarn run cypress run --spec cypress/e2e/HOTT-Shared/smokeTestCI.cy.js",
    "dev-tariff-frontend-smoketests": "CYPRESS_SPACE=DEVELOPMENT CYPRESS_BASE_URL=https://dev.trade-tariff.service.gov.uk yarn run cypress run --spec cypress/e2e/HOTT-Shared/devSmokeTestCI.cy.js",
    "dev-tariff-backend-smoketests": "CYPRESS_SPACE=DEVELOPMENT CYPRESS_BASE_URL=https://dev.trade-tariff.service.gov.uk yarn run cypress run --spec cypress/e2e/HOTT-Shared/devSmokeTestCI.cy.js",
    "dev-tariff-duty-calculator-smoketests": "CYPRESS_SPACE=DEVELOPMENT CYPRESS_BASE_URL=https://dev.trade-tariff.service.gov.uk yarn run cypress run --spec cypress/e2e/DutyCalculator/dcShared/dcSmokeTestCI.cy.js"
  }
}
